nr <- function(f, init_guess){
stopifnot(is.expression(f))
stopifnot(is.numeric(init_guess))
x <- init_guess
D_f <- D(f, "x")
counter <- 0
while(counter < 1000){
# print(paste("Guess: ", x))
if(eval(D_f) == 0){
print(paste("Initial guess of [", x, "] is problematic. Please try different guess."))
return(NULL)
}
x_next <- x - (eval(f)/eval(D_f))
if((x - x_next) == 0){
print("Exact root found.")
return(x_next)
}
x <- x_next
counter <- counter + 1
}
print("After 1000 iterations, root not found.")
return(NULL)
}
